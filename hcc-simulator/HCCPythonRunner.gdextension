windows.release.x86_64 = "res://../hcc-python-runner/target/release/hcc_python_runner.dll"
macos.debug =            "res://../hcc-python-runner/target/debug/libhcc_python_runner.dylib"
macos.release =          "res://../hcc-python-runner/target/release/libhcc-python-runner.dylib"
macos.debug.arm64 =      "res://../hcc-python-runner/target/debug/libhcc-python-runner.dylib"
macos.release.arm64 =    "res://../hcc-python-runner/target/release/libhcc-python-runner.dylib"